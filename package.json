{
  "name": "parroton",
  "description": "Parroton is a simple reinvestment platform on top of DeDust.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "check": "tsc --noEmit",
    "setup": "node ./scripts/setup.js",
    "prebuild": "node ./scripts/prebuild.js",
    "dev": "npm run setup && next dev",
    "build": "npm run prebuild && next build",
    "start": "next start",
    "lint": "next lint",
    "oxlint": "pnpx oxlint --import-plugin --jest-plugin --jsx-a11y-plugin --nextjs-plugin -D all -A nursery -c ./.eslintrc.json --deny-warnings",
    "g": "pnpx shadcn-ui add",
    "type-locales": "i18next-resources-for-ts interface -i ./src/i18n/locales/en -o ./@types/resources.d.ts",
    "dr:build": "npx declarative-routing build && node ./scripts/disable-linter-for-auto-generated-files.js",
    "dr:build:watch": "npx declarative-routing build --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "@dedust/apiary-v1": "^0.0.1",
    "@dedust/sdk": "^0.8.7",
    "@orbs-network/ton-access": "^2.3.3",
    "@parraton/sdk": "^1.0.4",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@ton/core": "^0.57.0",
    "@ton/ton": "^13.11.1",
    "@tonconnect/sdk": "^3.0.3",
    "@tonconnect/ui": "^2.0.6",
    "@tonconnect/ui-react": "^2.0.6",
    "@vkruglikov/react-telegram-web-app": "^2.1.9",
    "accept-language": "^3.0.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eruda": "^3.0.1",
    "formik": "^2.4.6",
    "i18next": "^23.11.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "query-string": "^9.0.0",
    "react": "^18",
    "react-cookie": "^7.1.4",
    "react-dom": "^18",
    "react-i18next": "^14.1.1",
    "react-slick": "^0.30.2",
    "rxjs": "^7.8.1",
    "sass": "^1.77.4",
    "sonner": "^1.4.41",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tonapi-sdk-js": "^1.0.10",
    "tonweb": "^0.0.66",
    "use-debounce": "^10.0.1",
    "zod": "^3.23.8",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "@types/telegram-web-app": "^7.2.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "declarative-routing": "^0.1.15",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-compat": "4.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.2.0",
    "eslint-plugin-jest-dom": "5.2.0",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-tailwindcss": "3.15.1",
    "eslint-plugin-testing-library": "6.2.2",
    "eslint-plugin-unicorn": "52.0.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "^9.0.11",
    "i18next-resources-for-ts": "^1.5.0",
    "oxlint": "^0.3.4",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
